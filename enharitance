void main() {
Dog dog=Dog(name:'german shaphrd',skincolor:'black');
print('name=${dog.name}');  
}

class Animal{
  String name;
  String skincolor;
  Animal({required this.name,required this.skincolor});
  void eat(){
    print('Animal is eating');
  }
  void sleep(){
    print('Animal is sleeping');
  }
}

class Dod extends Animal{
  Dog({required String name,required String skincolor}):super(name:name,skincolor:skincolor);
  void bark(){
    print('dog is barking');
  }
}
